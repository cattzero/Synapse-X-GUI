local function AFIEQFT_fake_script() -- SynapseX.Toggle 
	local script = Instance.new('LocalScript', SynapseX)

	local FrameObject = script.Parent.Parent.SynapseX
	local Open = false
	
	local UserInputService = game:GetService("UserInputService")
	
	UserInputService.InputBegan:Connect(function(Input)
		if Input.KeyCode == Enum.KeyCode.V then
			if Open then
				Open = false
				FrameObject.Visible = true
			else
				Open = true
				FrameObject.Visible = false
			end
		end
	end)
end
coroutine.wrap(AFIEQFT_fake_script)()
