local function ANNCD_fake_script() -- darkdexScript1.LocalScript 
	local script = Instance.new('LocalScript', darkdexScript1)

	local image1 = script.Parent.Parent.Parent.ImagesScript.ImageScript1
	local darkdex = script.Parent.Parent.Parent.Description.TextScript1
	local image2 = script.Parent.Parent.Parent.ImagesScript.ImageScript2
	local remotespy = script.Parent.Parent.Parent.Description.TextScript2
	
	script.Parent.MouseButton1Click:Connect(function()
		if image2.Visible == true then
			image2.Visible = false
		end
		if remotespy.Visible == true then
			remotespy.Visible = false
		end
		image1.Visible = true
		darkdex.Visible = true
	end)
end
coroutine.wrap(ANNCD_fake_script)()
